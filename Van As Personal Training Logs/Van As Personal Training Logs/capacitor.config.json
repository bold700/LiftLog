{
  "appId": "com.kennytimmer.vanaspersonaltraininglogs",
  "appName": "Van As Personal Training Logs",
  "webDir": "public",
  "server": {
    "androidScheme": "https",
    "iosScheme": "https"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "launchAutoHide": true,
      "backgroundColor": "#F2E4D3",
      "androidSplashResourceName": "splash",
      "androidScaleType": "CENTER_CROP",
      "showSpinner": false,
      "iosSpinnerStyle": "small",
      "spinnerColor": "#999999"
    },
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#F2E4D3"
    }
  },
  "packageClassList": [
    "AppPlugin",
    "HapticsPlugin",
    "KeyboardPlugin",
    "SplashScreenPlugin",
    "StatusBarPlugin"
  ]
}

